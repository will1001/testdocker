    "@types/react-redux": "^7.1.22",
    "@types/styled-components": "^5.1.21",
    "graphql": "^16.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "redux-logger": "^3.0.6",
